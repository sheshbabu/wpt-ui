  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "dotenv": "^4.0.0",
    "eslint": "^4.2.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "mocha": "^3.4.2",
    "node-mocks-http": "^1.6.4",
    "nyc": "^11.0.3",
    "prettier": "^1.5.3",
    "proxyquire": "^1.8.0",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
