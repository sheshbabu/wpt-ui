{
  "name": "wpt-ui",
  "version": "1.0.0",
  "description": "UI for WebPagetest",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run start:server' 'npm run start:client'",
    "start:server": "NODE_ENV=development node -e 'require(\"./index\").start()' | bunyan",
    "start:client": "react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_ENV=test mocha --recursive './!(node_modules|client)/**/*.spec.js'",
    "test:cov": "nyc npm test",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "postinstall": "npm run build"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "prettier --write", "git add"]
  },
  "nyc": {
    "reporter": ["lcov", "text", "html"]
  },
  "keywords": ["webpagetest"],
  "author": "Sheshbabu Chinnakonda",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.5.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.2.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "mocha": "^3.4.2",
    "node-mocks-http": "^1.6.4",
    "nyc": "^11.0.3",
    "prettier": "^1.5.3",
    "proxyquire": "^1.8.0",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "chart.js": "^2.6.0",
    "compression": "^1.7.0",
    "express": "^4.15.3",
    "knex": "^0.13.0",
    "material-ui": "^0.18.7",
    "moment": "^2.18.1",
    "pg": "^7.0.2",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-autobind": "^1.0.6",
    "react-chartjs-2": "^2.5.6",
    "react-dom": "^15.6.1",
    "react-scripts": "^1.0.11",
    "react-tap-event-plugin": "^2.0.1"
  },
  "proxy": "http://localhost:3000"
}
